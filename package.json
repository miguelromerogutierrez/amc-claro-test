{
  "name": "claro-example",
  "version": "0.3.0",
  "description": "Search and detail movie pages for Claro test.",
  "engines": {
    "node": ">4.2"
  },
  "scripts": {
    "start": "node scripts/server.js",
    "clean": "node scripts/clean.js",
    "asset": "node scripts/asset.js",
    "build": "rimraf dist && webpack --config ./webpack.production.config.js --progress --profile --colors",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^.+\\.(ico|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__mocks__/fileMock.js",
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    }
  },
  "author": "mromero <miguel.angel.romero.gtz@gmail.com>",
  "license": "BSD 3-clause",
  "dependencies": {
    "immutable": "^3.7.6",
    "react": "^15.0.0-rc.1",
    "react-dom": "^15.0.0-rc.1",
    "react-redux": "^4.4.0",
    "react-router": "^2.0.1",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.0",
    "redux": "^3.3.1",
    "redux-devtools": "^3.1.1",
    "redux-thunk": "^2.0.1",
    "svg-react-loader": "^0.4.5"
  },
  "devDependencies": {
    "babel-core": "^6.7.0",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^22.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.6.5",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-15": "^1.0.5",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^2.3.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^4.2.1",
    "estraverse-fb": "^1.3.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.4",
    "extract-text-webpack-plugin": "^1.0.1",
    "fetch-mock": "^5.0.0",
    "html-webpack-plugin": "^2.9.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "jest-css-modules": "^1.1.0",
    "jest-immutable-matchers": "^2.0.0",
    "jsdom": "11.5.1",
    "jsdom-global": "3.0.2",
    "ncp": "^2.0.0",
    "postcss": "^5.0.19",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-reporter": "^1.3.3",
    "postcss-url": "^5.1.1",
